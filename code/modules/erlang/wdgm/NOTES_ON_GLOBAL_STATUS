INIT
[WDGM285] -> 'WDGM_GLOBAL_STATUS_OK'

DEINIT
[WDGM286] if 'WDGM_GLOBAL_STATUS_OK' -> 'WDGM_GLOBAL_STATUS_DEACTIVATED'

MAIN
[WDGM078] 'WDGM_GLOBAL_STATUS_OK', ['WDGM_LOCAL_STATUS_OK'|| 'WDGM_LOCAL_STATUS_DEACTIVATED'] -> 'WDGM_GLOBAL_STATUS_OK'
[WDGM076] 'WDGM_GLOBAL_STATUS_OK', ['WDGM_LOCAL_STATUS_FAILED' && !'WDGM_LOCAL_STATUS_EXPIRED'] -> 'WDGM_GLOBAL_STATUS_FAILED'
[WDGM215] 'WDGM_GLOBAL_STATUS_OK', ['WDGM_LOCAL_STATUS_EXPIRED'], WdgMExpiredSupervisionCycleTol > 0 -> 'WDGM_GLOBAL_STATUS_EXPIRED'
[WDGM216] 'WDGM_GLOBAL_STATUS_OK', ['WDGM_LOCAL_STATUS_EXPIRED'], WdgMExpiredSupervisionCycleTol == 0 -> 'WDGM_GLOBAL_STATUS_STOPPED'
[WDGM217] 'WDGM_GLOBAL_STATUS_FAILED', ['WDGM_LOCAL_STATUS_FAILED' && !'WDGM_LOCAL_STATUS_EXPIRED'] -> 'WDGM_GLOBAL_STATUS_FAILED'
[WDGM218] 'WDGM_GLOBAL_STATUS_FAILED', ['WDGM_LOCAL_STATUS_OK'|| 'WDGM_LOCAL_STATUS_DEACTIVATED'] -> 'WDGM_GLOBAL_STATUS_OK'
[WDGM077] 'WDGM_GLOBAL_STATUS_FAILED', ['WDGM_LOCAL_STATUS_EXPIRED'], WdgMExpiredSupervisionCycleTol > 0 -> 'WDGM_GLOBAL_STATUS_EXPIRED'
[WDGM117] 'WDGM_GLOBAL_STATUS_FAILED', ['WDGM_LOCAL_STATUS_EXPIRED'], WdgMExpiredSupervisionCycleTol == 0 -> 'WDGM_GLOBAL_STATUS_STOPPED'
[WDGM219] 'WDGM_GLOBAL_STATUS_EXPIRED', ['WDGM_LOCAL_STATUS_EXPIRED'], EXPIRED_CYCLES <= WdgMExpiredSupervisionCycleTol -> EXPIRED_CYCLES++, 'WDGM_GLOBAL_STATUS_EXPIRED'
[WDGM220] 'WDGM_GLOBAL_STATUS_EXPIRED', ['WDGM_LOCAL_STATUS_EXPIRED'], EXPIRED_CYCLES > WdgMExpiredSupervisionCycleTol -> 'WDGM_GLOBAL_STATUS_STOPPED'
[WDGM221] 'WDGM_GLOBAL_STATUS_STOPPED' -> 'WDGM_GLOBAL_STATUS_STOPPED'

OTHER
[WDGM139] if wdgif_setmode fails assume global supervision failure -> 'WDGM_GLOBAL_STATUS_STOPPED'
